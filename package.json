{
  "name": "@thesweaterguys/nucleus-style",
  "version": "0.9.0",
  "description": "React component library for The Sweater Guys",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "browser": "dist/index.umd.js",
  "scripts": {
    "build": "rollup -c",
    "deploy": "build-storybook -c .storybook -o build",
    "format": "prettier --write \"**/*.{js,jsx,md,mdx}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,md,mdx}\" && echo 'Format check complete.'",
    "lint": "eslint \"**/*.{js,jsx}\" && echo 'Lint complete.'",
    "release": "auto changelog && npm run version && auto release",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9000 -c .storybook",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "version": "npm version `auto version` -m 'Bump version to: %s [skip ci]'"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheSweaterGuys/nucleus-style.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheSweaterGuys/nucleus-style/issues"
  },
  "homepage": "https://github.com/TheSweaterGuys/nucleus-style#readme",
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "auto": "^9.50.11",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.2.2",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^26.2.2",
    "jest-styled-components": "^7.0.2",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "react-is": "^16.13.1",
    "rollup": "^2.22.1",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "standard-version": "^8.0.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "styled-components": "5.x"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
